{
  "name": "oc-angular-template",
  "version": "0.0.0",
  "scripts": {
    "ng": "./node_modules/.bin/ng",
    "start": "./node_modules/.bin/ng serve",
    "build": "./node_modules/.bin/ng build",
    "build-prod": "./node_modules/.bin/ng build --prod",
    "test": "npm run test:lib",
    "test:lib": "jest --config ./projects/oc-ng-common-component/jest.config.js --collectCoverage",
    "sass-lint": "node_modules/.bin/sass-lint -c .scss-lint.yml",
    "lint": "./node_modules/.bin/ng lint",
    "e2e": "./node_modules/.bin/ng e2e",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006 -s projects/oc-ng-common-component/",
    "storybook-withoutdocs": "start-storybook -p 6006 -s projects/",
    "build-storybook": "build-storybook",
    "oc-ng-common-component-pack": "cd dist/oc-ng-common-component/ && npm pack",
    "oc-ng-common-component-up-version": "cd projects/oc-ng-common-component/ && npm --no-git-tag-version version prerelease",
    "oc-ng-common-component-watch": "./node_modules/.bin/ng build oc-ng-common-component --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.13",
    "@angular/common": "~9.1.1",
    "@angular/compiler": "~9.1.1",
