  "devDependencies": {
    "@angular-builders/jest": "^10.0.1",
    "@angular-devkit/build-angular": "^0.1100.1",
    "@angular-devkit/build-ng-packagr": "~0.900.7",
    "@angular/cli": "~9.1.1",
    "@angular/compiler-cli": "~9.1.1",
    "@angular/language-service": "~9.1.1",
    "@babel/core": "^7.10.5",
    "@storybook/addon-a11y": "^6.1.6",
    "@storybook/addon-actions": "^6.1.6",
    "@storybook/addon-controls": "^6.1.6",
    "@storybook/addon-docs": "^6.1.6",
    "@storybook/addon-links": "^6.1.6",
    "@storybook/addon-storysource": "^6.1.6",
    "@storybook/addon-viewport": "^6.1.6",
    "@storybook/addons": "^6.1.6",
    "@storybook/angular": "^6.1.6",
    "@tinymce/tinymce-angular": "^4.2.0",
    "@types/chart.js": "^2.9.20",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^26.0.14",
    "@types/node": "^12.11.1",
    "autoprefixer": "^8.6.5",
    "babel-loader": "^8.1.0",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^26.5.3",
    "jest-preset-angular": "^8.3.1",
    "jwt-decode": "^3.1.2",
    "ng-packagr": "^9.0.0",
    "protractor": "^7.0.0",
    "sass-lint": "^1.13.1",
    "ts-jest": "^26.4.1",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.5"
  }
