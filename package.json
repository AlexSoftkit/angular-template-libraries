    "@angular/localize": "^9.1.1",
    "@angular/platform-browser": "~9.1.1",
    "@angular/platform-browser-dynamic": "~9.1.1",
    "@angular/router": "~9.1.1",
    "@compodoc/compodoc": "^1.1.11",
    "@ng-bootstrap/ng-bootstrap": "^6.0.2",
    "@ng-select/ng-select": "^4.0.0",
    "@tinymce/tinymce-angular": "^4.2.0",
    "angular-svg-icon": "^11.0.0",
    "bootstrap": "^4.4.1",
    "chart.js": "^2.9.3",
    "ngx-color-picker": "^10.1.0",
    "ngx-embed-video": "^1.0.4",
    "ngx-image-cropper": "^3.2.1",
    "ngx-infinite-scroll": "^10.0.0",
    "ngx-owl-carousel-o": "^4.0.0",
    "ngx-spinner": "^9.0.2",
    "oc-ng-common-component": "file:dist/oc-ng-common-component",
    "oc-ng-common-service": "0.0.2-49",
    "react": "^16.13.1",
    "react-is": "^16.13.1",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
